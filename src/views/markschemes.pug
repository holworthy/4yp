doctype html
html(lang="en")
	head
		include includes/head.pug
	body
		include includes/header.pug
		.container.p1.m1-top
			h1.m1-bottom Markschemes
			.markschemes
				- for(let i = 0; i < markschemes.length; i++)
						a.markscheme(href="/markscheme/"+markschemes[i].id)
							h2 #{markschemes[i].name}
							p Created by ?
				- if(markschemes.length == 0)
					p No markschemes exist yet!
			a.m1-top(href="/markscheme/new")
				h2 Or Create A New Markscheme 
			
